<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <RootNamespace>jp.co.systembase.barcode.skia</RootNamespace>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <AssemblyName>systembase.barcode.skia</AssemblyName>
    <PackageId>jp.co.systembase.barcode.skia</PackageId>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>barcode.skia.snk</AssemblyOriginatorKeyFile>
    <Version>1.7</Version>
    <AssemblyVersion>1.7.0.0</AssemblyVersion>
    <FileVersion>1.7.0.0</FileVersion>
    <Authors>systembase</Authors>
    <Company>systembase</Company>
    <Product>jp.co.systembase.barcode.skia</Product>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="NotoSans-Regular.ttf" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="NotoSans-Regular.ttf" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="SkiaSharp" Version="2.88.3" />
  </ItemGroup>

</Project>
